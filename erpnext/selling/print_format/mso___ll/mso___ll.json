 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header,\n\t render_field -%}\n\t \n\t{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n\t\t<table  style=\"border-bottom:1px solid lightgray; margin:0px !important; padding:0px !important;\">\n\t\t\t  <tbody>\n\t\t\t\t\t<tr  style=\"padding:0px !important; margin:0px !important\">\n\t\t\t\t\t\t<td  style=\"padding:0px !important; margin:0px !important\" width=\"500px\" class=\"text-left\">\n\t\t\t\t\t\t\t<img style=\"width:200px !important\" src=\"http://erp.sundinepro.com:3500/files/Sundine_LOGO_FINAL_large.png\" width=\"42%\" /> <br/>\n\t\t\t\t\t\t\t<p style=\"font-size:11px;\">6075 Kestrel Rd, Mississauga, ON L5T 1Y8 Canada <br/>\n\t\t\t\t\t\t\tPh: 905 364 1600 | Fax: 416 342 1763\t<br/>\t\n\t\t\t\t\t\t\tEmail: sales@sundineproduce.com  |\twww.sundineproduce.com</p>\t\n\t\t\t\t\t\t</td>\n\t\t\t\t\t\t<td   style=\"padding:0px !important; margin:0px !important\" width=\"500px\"  align=\"right\">\n\t\t\t\t\t\t\t<br /><h2 style=\"margin-bottom:0px !important; padding-bottom:0px !important;\">SALE INVOICE</h2><b> {{ doc.name }}</b>\n\t\t\t\t\t\t\t<p style=\"font-size:12px !important;\">\n\t\t\t\t\t\t\tDate: <b>{{ doc.posting_date }}</b><br/>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\tTerms: <b>{{ doc.tc_name or \"\" }}</b><br/>\n\t\t\t\t\t\t\tCust. PO # <b>{{ doc.po_no or \"\" }}</b>\n\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t</td>\n\t\t\t\t\t</tr>\n\t\t\t\t</tbody>\n\t\t\t</table>\n\t{%- endmacro -%}\n\t{{ add_header(1, layout|len,doc,letter_head, no_letterhead) }}\n \n\t\t<table class=\"table table-condensed table-hover table-bordered\" style=\"border:0px !important; align:center; margin-top:0px !important; padding-top:0px !important;\">\n\t\t  <tbody>\n\t\t\t\t<tr>\n\t\t\t\t\t<td  style=\"padding:1px !important; border:0px !important; \" width=\"600px\" class=\"text-left\">\n\t\t\t\t\t\t<p style=\"font-size:14px !important; padding: 5px 0px;\">\n\t\t\t\t\t\t<b>{{ doc.customer_name }} </b> </p>\n\t\t\t\t\t\t<p style=\"font-size:12px !important;\"><b>Billing Address:</b><br/>\n\t\t\t\t\t\t{{ doc.address_display or \"\" }}\n\t\t\t\t\t\t</p>\n\t\t\t\t\t</td>\n\t\t\t\t\t<td  style=\"padding:1px !important; border:0px !important; width:400px;\"  class=\"text-left\">\n\t\t\t\t\t<p style=\"font-size:12px !important;\">\n\t\t\t\t\t\t<br/><br/>\n\t\t\t\t\t\t<b>Shipping Address:</b><br/>\n\t\t\t\t\t\t \n\t\t\t\t\t\t{{ doc.address_display or \"\" }}\n\t\t\t\t\t\t</p>\n\t\t\t\t\t</td>\n\t\t\t\t</tr>\n\t\t\t</tbody>\n\t\t</table>\n<table class=\"table table-condensed table-hover table-bordered\" style=\"width:100% !important; margin-top:5px !important; border-collapse: collapse;\">\n  <tbody>\n\t\t <tr style=\"border:none\">\n\t\t\t<th  style=\"border: 1px solid #d1d8dd; font-size:12px; text-align:center;\"  class=\"text-center\">Qty</th>\n\t\t\t<th  style=\"border: 1px solid #d1d8dd; font-size:12px; text-align:left;\"  class=\"text-left\">Item</th>\n\t\t\t<th  style=\"border: 1px solid #d1d8dd; font-size:12px; text-align:center;\"  class=\"text-center\">Lbs/Box</th>\n\t\t\t<th  style=\"border: 1px solid #d1d8dd; font-size:12px; text-align:center;\"  class=\"text-center\">Unit Price</th>\n\t\t\t<th  style=\"border: 1px solid #d1d8dd; font-size:12px; text-align:right;\"  class=\"text-right\">Amount</th>\n        </tr>\n\t\t{% set count={'total':0} %}\n\t\t\t{%- for row in doc.items -%}\n\t\t\t\t{% if row.qty >= 0 %}\n\t\t\t\t\t<tr>\n\t\t\t\t\t\t<td style=\"width: 15%; font-size:10px; text-align: center;  padding:2px !important; border: 1px solid #d1d8dd; \"><b>{{ row.qty or '' }} </b> </td>\n\t\t\t\t\t\t<td style=\"width: 35%; font-size:10px; text-align: left; padding:2px !important; border: 1px solid #d1d8dd; \">{{ row.item_name or ''}}</td>\n\t\t\t\t\t\t<td style=\"width: 15%; font-size:10px; text-align: center; padding:2px !important; border: 1px solid #d1d8dd; \">{{ row.weight_lbs }}</td>\n\t\t\t\t\t\t<td style=\"width:15%; font-size:10px; text-align: center; padding:2px !important; border: 1px solid #d1d8dd; \">{{ '%0.2f'|format(row.rate|float) }}</td>\n\t\t\t\t\t   <td style=\"width: 20%; font-size:10px; text-align: right; padding:2px !important; border: 1px solid #d1d8dd; \">{{ '%0.2f'|format(row.amount|float)}}</td>\n\t\t\t\t\t</tr>\n\t\t\t\t\t{% set total1 = count.total + 1 %}\n\t\t\t\t\t\t{%-   if count.update({'total':total1 }) %}{%- endif %} \n\t\t\t\t{% endif %}\n\t\t\t{%- endfor -%}\n\t\t\t\t{%- for row in range(20-count.total) -%}\n\n\t\t\t\t<tr style=\" border:0px !important; height:20px !important\">\n\t\t\t\t\t<td style=\"width: 15%;text-align: center;  border:0px !important\">\n\t\t\t\t\t</td>\n\t\t\t\t\t<td style=\"width: 30%; text-align: left; border:0px !important\"></td>\n\t\t\t\t\t<td style=\"width: 15%; text-align: center;  border:0px !important\"></td>\n\t\t\t\t\t<td style=\"width: 15%; text-align: center;  border:0px !important\"></td>\n\t\t\t\t   <td style=\"width: 20%; text-align: center;  border:0px !important\"></td>\n\t\t\t\t</tr>\n\n\n\t\t\t{%- endfor -%}\n\t\t</tbody>\n</table>\n<div id=\"footer-html\" style=\"margin:0px !important; padding:0px !important; \" class=\"visible-pdf\">\n\t\t<table class=\"table table-condensed table-hover table-bordered\" style=\"margin:0px !important; border-collapse: collapse;\">\n\t\t  <tbody>\n\t\t\t\t <tr style=\"border:none\">\n\t\t\t\t\t<th  style=\"width: 10%; border:1px solid #d1d8dd;  font-size:12px;  text-align:center;\"  class=\"text-center\">{{ doc.get_formatted(\"total_boxes\") or '' }}</th>\n\t\t\t\t\t<th  style=\"width: 30%; text-align:left;  font-size:12px;\"  class=\"text-left\"> Total Boxes</th>\n\t\t\t\t\t<th  colspan=\"2\" style=\"width:15%;  font-size:12px; text-align:right;\"  class=\"text-center\">Sub Total:</th>\n\t\t\t\t\t<th  style=\"width:15%;  font-size:12px; border: 1px solid #d1d8dd; text-align:right;\"  class=\"text-right\">{{ doc.get_formatted(\"total\") or '' }}</th>\n\t\t\t\t</tr>\n\t\t\t</tbody>\n\t\t</table>\n\t\t<table class=\"table table-condensed table-hover table-bordered\" style=\"border:0px !important; align:center;\">\n\t\t  <tbody>\n\t\t\t\t<tr>\n\t\t\t\t\t<td  style=\"padding:1px !important; border:0px !important; \" width=\"600px\" class=\"text-left\">\n\t\t\t\t\t\t<br/>\n\t\t\t\t\t\t<hr style=\"border-top:1px dashed #000; color:#ffffff;\" />\n\t\t\t\t\t\t<p align=\"center\">Receiver Signature</p>\n\t\t\t\t\t\t<p style=\"font-size:10px;\">All the above goods received are fresh and in good condition.<br/>\t\t\t\t\n\t\t\t\t\t\tNo deductions or price adjustment will be accepted unless authorized by the company.<br/>\n\t\t\t\t\t\tAll claims must be in writing within 24 hrs of receipt of above goods.</p>\t\n\t\t\t\t\t</td>\n\t\t\t\t\t<td  style=\"padding:1px !important; border:0px !important; \" width=\"150px\" class=\"text-left\">\n\t\t\t\t\t\t\n\t\t\t\t\t</td>\n\t\t\t\t\t<td  style=\"padding:1px !important; border:0px !important; \" width=\"250px\"  class=\"text-left\">\n\t\t\t\t\t\t<br /><br />\n\t\t\t\t\t\t<table class=\"table table-condensed table-hover table-bordered\" style=\"border:0px !important; align:center; font-size:12px !important\">\n\t\t\t\t\t\t\t<tbody>\n\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t<td class=\"text-right\" style=\" padding:2px !important;  border:none\">Delivery Charges:</td>\n\t\t\t\t\t\t\t\t\t<td class=\"text-right\" style=\" padding:2px !important;  border:none\">{{doc.delivery_charges or ''}} </td>\t\n\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t<td class=\"text-right\" style=\"  padding:2px !important;  border:none\">HST:</td>\n\t\t\t\t\t\t\t\t\t<td class=\"text-right\" style=\" padding:2px !important;  border:none\">{{doc.total_taxes_and_charges or ''}} </td>\t\n\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t<td class=\"text-right\" style=\"border:none; font-size:14px;  padding:2px !important; \"><b>Grand Total:<b></td>\n\t\t\t\t\t\t\t\t\t<td class=\"text-right\" style=\"border:none; font-size:14px;  padding:2px !important; \"><b>{{doc.get_formatted(\"grand_total\") or '' }}</b></td>\t\n\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t</tbody>\n\t\t\t\t\t\t</table>\n\t\t\t\t\t</td>\n\t\t\t\t</tr>\n\t\t\t</tbody>\n\t\t</table>\n\t\t\n\t\t\n\t</div>\n\t\n\t{% if not no_letterhead and footer %}\n\t\t\n\t\t{% endif %}", 
